<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context="cc.seeed.stu.seeedhackathon12.MainActivity"
    tools:showIn="@layout/activity_main"
    android:background="#464444">

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageButton_1"
        android:background="@drawable/sleep"
        android:src="@drawable/sleep" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageButton_2"
        android:src="@drawable/work"
        android:background="@drawable/work"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageButton_3"
        android:background="@drawable/pill"
        android:layout_alignParentTop="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageButton_6"
        android:layout_marginTop="35dp"
        android:background="@drawable/pregant"
        android:layout_below="@+id/imageButton_1"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageButton_5"
        android:background="@drawable/love"
        android:layout_alignTop="@+id/imageButton_6"
        android:layout_alignLeft="@+id/imageButton_2"
        android:layout_alignStart="@+id/imageButton_2" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageButton_4"
        android:background="@drawable/money"
        android:layout_alignTop="@+id/imageButton_5"
        android:layout_alignLeft="@+id/imageButton_3"
        android:layout_alignStart="@+id/imageButton_3" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageButton_7"
        android:layout_marginTop="32dp"
        android:background="@drawable/bike"
        android:layout_below="@+id/imageButton_6"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageButton_8"
        android:background="@drawable/hill"
        android:layout_alignTop="@+id/imageButton_7"
        android:layout_alignLeft="@+id/imageButton_5"
        android:layout_alignStart="@+id/imageButton_5" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageButton_9"
        android:layout_alignTop="@+id/imageButton_8"
        android:layout_alignLeft="@+id/imageButton_4"
        android:layout_alignStart="@+id/imageButton_4"
        android:background="@drawable/mojang" />
</RelativeLayout>
